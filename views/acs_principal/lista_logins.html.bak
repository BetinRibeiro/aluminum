{{extend 'layout.html'}}
<div class="py-5" style="">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <h5 class="" >Usuários do Sistema (Administradores, Chefes e Cobradores)</h5>
      </div>
          <div class="row">
        <div class="col-md-12">
          <a class="btn btn-link" href="{{=URL('acs_principal','index', args=empresa.id*12)}}" style="color:#3b5998">
            <i class="fa fa-fw fa-1x py-1 fa-arrow-left"></i> Voltar </a>
        </div>
      </div>
      </div>
      
<div class="row">
            <div class="col-md-12">
              
                
              <div class="table-responsive">
                <table class="table table-striped table-borderless">
                  <thead>
                    <tr>
                      <th scope="col">Nº</th>
                      <th scope="col">Nome</th>
                      <th scope="col">Senha</th>
                      <th scope="col">Opções</th>
                    </tr>
                  </thead>
                  <tbody>{{a=0}}
                      {{for row in row:}}
                    {{a+=1}}
                    <tr>
                      <th scope="row">{{=a}}</th>
                      <td>{{=row.nome}}</td>
                      <td>{{=row.telefone}}</td>
                      <td><a class="btn btn-link" href="#" style="color:#3b5998""><i class="fa fa-fw fa-1x py-1 fa-bars"></i></a></td>
                    </tr>
                      {{pass}}
                  </tbody>
                </table>
              </div>
              </div>
              </div>
              </div>
              </div>
